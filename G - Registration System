void JustA_GININ_TryToSolve()
{
    ll n;
    cin>>n;
    map<string,ll> ms;
    string a;
    loop0(n)
    {
       cin>>a;
        if (!ms.empty())
        {
            if (ms[a]==0)
            {
               cout<<"OK";el;
            }else
            {
                cout<<a<<ms[a];el;
            }
            ms[a]++;
        }else
        {
            ms.emplace(a,0);
            cout<<"OK";el;
            ms[a]++;
        }
    }
}
