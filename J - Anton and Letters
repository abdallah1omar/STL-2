void JustA_GININ_TryToSolve()
{
    vector<char>ar(1001,'0');
    cin>>ar[0];
    ll i=0;
    while (ar[i]!='}')
    {
        i++;
        cin>>ar[i];
    }
    //cout<<i<<' ';el;
    set<char>s;
    for (ll i=0;i<ar.size();i++)
    {
       // cout<<ar[i]<<' ';
        if (ar[i]!='}'&&ar[i]!='{'&&ar[i]!=','&&ar[i]!=' '&&ar[i]!='0')
        {
            s.insert(ar[i]);
        }
    }
    cout<<s.size();

}
