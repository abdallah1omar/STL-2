void JustA_GININ_TryToSolve()
{
    // GININ IN FOURTH NINGA WAR
   ll n;
    cin>>n;
    set<char>mp;
    vector<char>ar(n);
    loop0(n)
    {
        cin>>ar[i];
        if (isupper(ar[i]))
        {
            ar[i]=tolower(ar[i]);
        }
      //  cout<<ar[i]<<' ';
        mp.insert(ar[i]);
    }
    //el;
    if (mp.size()>=26)
    {
        yes;el;
    }else
    {
        no;el;
    }
}
